option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "PubBrain.settings"
    "PYTHONPATH": "/opt/python/current/app/PubBrain:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: PubBrain/PubBrain/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"